    <!-- Barre de progression pour le creneau de 16h -->

    <ProgressBar
        android:id="@+id/progressBar9"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/seizeh"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
        android:progressDrawable="@drawable/custom_progress_bar_horizontal"
        android:secondaryProgress="70" />

    <!-- Barre de progression pour le creneau de 17h -->

    <ProgressBar
        android:id="@+id/progressBar10"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/dixsepth"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
