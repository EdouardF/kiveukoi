        android:secondaryProgress="70" />

    <!-- Barre de progression pour le creneau de 12h -->

    <ProgressBar
        android:id="@+id/progressBar5"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/douzeh"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
        android:progressDrawable="@drawable/custom_progress_bar_horizontal"
        android:secondaryProgress="70" />

    <!-- Barre de progression pour le creneau de 13h -->

    <ProgressBar
        android:id="@+id/progressBar6"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/treizeh"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
        android:progressDrawable="@drawable/custom_progress_bar_horizontal"
        android:secondaryProgress="70" />

    <!-- Barre de progression pour le creneau de 14h -->

    <ProgressBar
        android:id="@+id/progressBar7"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/quatorzeh"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
        android:progressDrawable="@drawable/custom_progress_bar_horizontal"
        android:secondaryProgress="70" />

    <!-- Barre de progression pour le creneau de 15h -->

    <ProgressBar
        android:id="@+id/progressBar8"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/quinzeh"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
