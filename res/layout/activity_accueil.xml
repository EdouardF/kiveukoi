    <!-- affichage 18h -->

    <TextView
        android:id="@+id/dixhuith"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/dixsepth"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="25dp"
        android:text="@string/dixhuith"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <!-- Barre de progression pour le creneau de 8h -->

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textViewAlignementProgressBar"
        android:layout_alignTop="@+id/huith"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
        android:progressDrawable="@drawable/custom_progress_bar_horizontal"
        android:secondaryProgress="70" />

    <!-- Barre de progression pour le creneau de 9h -->

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/neufh"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
        android:progressDrawable="@drawable/custom_progress_bar_horizontal"
        android:secondaryProgress="70" />

    <!-- Barre de progression pour le creneau de 10h -->

    <ProgressBar
        android:id="@+id/progressBar3"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/progressBar1"
        android:layout_alignTop="@+id/dixh"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/dixh"
        android:max="100"
        android:minHeight="16dp"
        android:minWidth="30dp"
        android:progress="8"
