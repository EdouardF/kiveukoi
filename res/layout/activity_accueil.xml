        android:layout_below="@+id/quatorzeh"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="25dp"
        android:text="@string/quinzeh"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <!-- affichage 16h -->

    <TextView
        android:id="@+id/seizeh"
